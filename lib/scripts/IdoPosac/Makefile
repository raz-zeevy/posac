LINKER64 = c:\MinGW64\bin\gfortran
LINKER32 = c:\MinGW\bin\gfortran
LOPTS = -static
FCOMP64 = c:\MinGW64\bin\gfortran
FCOMP32 = c:\MinGW\bin\gfortran
COPTS = -Wall -ffixed-form -std=legacy

ALL_EXEC = TNWPOSAC.exe  SLSA1.exe  SLSA2.exe  SSHEMOR.exe  POSACSEP.exe POSXTSP1.exe POSXTSP2.exe POSXTSP3.exe 

all : 32b 64b
#	  copy TSPOSAC.exe TPOSAC.exe
#	  del TSPOSAC.exe

32b: $(ALL_EXEC:.exe=_32.exe)
64b: $(ALL_EXEC:.exe=_64.exe)
	  
clean :
	del *.obj *.exe

%_32.exe : %_32.obj
	$(LINKER32) $(LOPTS) $^ -o $@

%_64.exe : %_64.obj
	$(LINKER64) $(LOPTS) $^ -o $@

%_32.obj : %.FOR
	$(FCOMP32) $(COPTS) $< -c -o $@

%_64.obj : %.FOR
	$(FCOMP64) $(COPTS) $< -c -o $@

POSACSEP_32.exe  : GRAPH_SHELL_32.obj

POSXTSP1_32.exe  : GRAPH_SHELL_32.obj

POSXTSP2_32.exe  : GRAPH_SHELL_32.obj

POSXTSP3_32.exe  : GRAPH_SHELL_32.obj

TNWPOSAC_32.exe : TSPOSAC_32.obj T0POSAC_32.obj T1POSAC_32.obj T2POSAC_32.obj \
             T3POSAC_32.obj T4POSAC_32.obj T5POSAC_32.obj T6POSAC_32.obj T7POSAC_32.obj \
             T8POSAC_32.obj T9POSAC_32.obj


