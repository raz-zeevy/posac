LINKER = c:\MinGW\bin\gfortran
LOPTS =
FCOMP = c:\MinGW\bin\gfortran
COPTS = -Wall -ffixed-form -std=legacy

all : TSPOSAC.exe  SLSA1.exe  SLSA2.exe  SSHEMOR.exe  POSACSEP.exe \
      POSXTSP1.exe POSXTSP2.exe POSXTSP3.exe
	  copy TSPOSAC.exe TPOSAC.exe
	  del TSPOSAC.exe

clean :
	del *.obj *.exe

%.exe : %.obj
	$(LINKER) $(LOPTS) $^ -o $@

%.obj : %.FOR
	$(FCOMP) $(COPTS) $< -c -o $@

POSACSEP.exe  : GRAPH_SHELL.obj

POSXTSP1.exe  : GRAPH_SHELL.obj

POSXTSP2.exe  : GRAPH_SHELL.obj

POSXTSP3.exe  : GRAPH_SHELL.obj

